@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix leakage-test: <https://github.com/GAIA-X4PLC-AAD/ontology-management-base/tree/main/leakage-test> .
@prefix general: <https://github.com/GAIA-X4PLC-AAD/ontology-management-base/tree/main/general/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

leakage-test:LeakageTestShape
    a sh:NodeShape ;
    sh:property [ sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:node general:GeneralShape ;
        sh:path leakage-test:general ],
	[ sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:node leakage-test:MachineParameterShape ;
        sh:path leakage-test:MachineParameter ] ,
    [ sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:node leakage-test:ProcessParameterShape ;
        sh:path leakage-test:ProcessParameter ] ,
    [ sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:node leakage-test:CodingDownholderShape ;
        sh:path leakage-test:CodingDownholder ] ,
    [ sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:node leakage-test:CodingTestAdapterShape ;
        sh:path leakage-test:CodingTestAdapter ] ,
    [ sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:node leakage-test:CodingRFIDShape ;
        sh:path leakage-test:CodingRFID ] ,
    [ sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:node leakage-test:MaximumPressureShape;
        sh:path leakage-test:MaximumPressure ] ,
    [ sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:node leakage-test:MinimumPressureShape;
        sh:path leakage-test:MinimumPressure ]; 
    sh:targetClass leakage-test:LeakageTest .

leakage-test:MachineParameterShape
    a sh:NodeShape ;
    sh:property [ sh:minCount 1 ;
	sh:maxCount 1 ] ;
    sh:targetClass leakage-test:MachineParameter .

    leakage-test:ProcessParameterShape
    a sh:NodeShape ;
    sh:property [ sh:minCount 1 ;
        sh:maxCount 1 ] ;
    sh:targetClass leakage-test:ProcessParameter .

leakage-test:DownholderShape
    a sh:NodeShape ;
    sh:property [ skos:example "1" ;
        sh:datatype xsd:integer ;
	sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Validation of setpoint failed!"@en ;
        sh:name "setpoint"@en ;
        sh:path leakage-test:setpoint ] ;
    sh:targetClass leakage-test:Downholder .

leakage-test:CodingTestAdapterShape
    a sh:NodeShape ;
    sh:property [ skos:example "1" ;
        sh:datatype xsd:integer ;
	sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Validation of setpoint failed!"@en ;
        sh:name "setpoint"@en ;
        sh:path leakage-test:setpoint ] ; 
    sh:targetClass leakage-test:CodingTestAdapter .

leakage-test:CodingRFIDShape
    a sh:NodeShape ;
    sh:property [ skos:example "16" ;
        sh:datatype xsd:integer ;
	sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Validation of setpoint failed!"@en ;
        sh:name "setpoint"@en ;
        sh:path leakage-test:setpoint ] ; 
    sh:targetClass leakage-test:CodingRFID .

leakage-test:MaximumPressureShape
    a sh:NodeShape ;
    sh:property [ skos:example "bar" ;
        sh:datatype xsd:string ;
	sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Validation of unit failed!"@en ;
        sh:name "unit"@en ;
        sh:path leakage-test:unit ] ,
    [ skos:example "0" ;
        sh:datatype xsd:decimal ;
	sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Validation of upperTolerance failed!"@en ;
        sh:name "upperTolerance"@en ;
        sh:path leakage-test:upperTolerance ] ,
     [ skos:example "2.7";
        sh:datatype xsd:decimal ;
	sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Validation of setpoint failed!"@en ;
        sh:name "setpoint"@en ;
        sh:path leakage-test:setpoint ] ,
    [ skos:example "0" ;
        sh:datatype xsd:decimal ;
	sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Validation of lowerTolerance failed!"@en ;
        sh:name "lowerTolerance"@en ;
        sh:path leakage-test:lowerTolerance ] ; 
    sh:targetClass leakage-test:MaximumPressure .
    
leakage-test:MinimumPressureShape
    a sh:NodeShape ;
    sh:property [ skos:example "bar" ;
        sh:datatype xsd:string ;
	sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Validation of unit failed!"@en ;
        sh:name "unit"@en ;
        sh:path leakage-test:unit ] ,
    [ skos:example "0" ;
        sh:datatype xsd:decimal ;
	sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Validation of upperTolerance failed!"@en ;
        sh:name "upperTolerance"@en ;
        sh:path leakage-test:upperTolerance ] ,
    [ skos:example "2.7";
        sh:datatype xsd:decimal ;
	sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Validation of setpoint failed!"@en ;
        sh:name "setpoint"@en ;
        sh:path leakage-test:setpoint ] ,
    [ skos:example "0" ;
        sh:datatype xsd:decimal ;
	sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Validation of lowerTolerance failed!"@en ; 
        sh:name "lowerTolerance"@en ;
        sh:path leakage-test:lowerTolerance ] ;
    sh:targetClass leakage-test:MinimumPressure .
