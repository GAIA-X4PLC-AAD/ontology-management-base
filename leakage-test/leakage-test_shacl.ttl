@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix leakage-test: <https://github.com/GAIA-X4PLC-AAD/ontology-management-base/tree/leakage-test/leakage-test> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix openlabel: <https://openlabel.asam.net/V1-0-0/ontologies/> .

leakage-test:LeakageTestShape
    a sh:NodeShape ;
    sh:property [ sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:node leakage-test:MaschinenparameterShape ;
        sh:path leakage-test:Maschinenparameter ] ,
    [ sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:node leakage-test:ProzessparameterShape ;
        sh:path leakage-test:Prozessparameter ] ,
    [ sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:node leakage-test:CodierungNiederhalterShape ;
        sh:path leakage-test:CodierungNiederhalter ] ,
    [ sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:node leakage-test:CodierungPruefadapterShape ;
        sh:path leakage-test:CodierungPruefadapter ] ,
    [ sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:node leakage-test:CodierungRFIDShape ;
        sh:path leakage-test:CodierungRFID ] ,
    [ sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:node leakage-test:MaximalerDruckShape;
        sh:path leakage-test:MaximalerDruck ] ,
    [ sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:node leakage-test:MinimalerDruckShape;
        sh:path leakage-test:MinimalerDruck ]; 
    sh:targetClass leakage-test:LeakageTest .

leakage-test:CodierungNiederhalterShape
    a sh:NodeShape ;
    sh:property [ skos:example "1" ;
        sh:datatype xsd:integer ;
	sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Validation of hatSollwert failed!"@en ;
        sh:name "hatSollwert" ;
        sh:path leakage-test:hatSollwert ] ;
    sh:targetClass leakage-test:CodierungNiederhalter .

leakage-test:CodierungPruefadapterShape
    a sh:NodeShape ;
    sh:property [ skos:example "1" ;
        sh:datatype xsd:integer ;
	sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Validation of hatSollwert failed!"@en ;
        sh:name "hatSollwert" ;
        sh:path leakage-test:hatSollwert ] ; 
    sh:targetClass leakage-test:CodierungPruefadapter .

leakage-test:CodierungRFIDShape
    a sh:NodeShape ;
    sh:property [ skos:example "16" ;
        sh:datatype xsd:integer ;
	sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Validation of hatSollwert failed!"@en ;
        sh:name "hatSollwert" ;
        sh:path leakage-test:hatSollwert ] ; 
    sh:targetClass leakage-test:CodierungRFID .

leakage-test:MaximalerDruckShape
    a sh:NodeShape ;
    sh:property [ skos:example "bar" ;
        sh:datatype xsd:string ;
	sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Validation of hatEinheit failed!"@en ;
        sh:name "hatEinheit" ;
        sh:path leakage-test:hatEinheit ] ,
    [ skos:example "0" ;
        sh:datatype xsd:decimal ;
	sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Validation of hatObereToleranz failed!"@en ;
        sh:name "hatObereToleranz" ;
        sh:path leakage-test:hatObereToleranz ] ,
     [ skos:example "2.7";
        sh:datatype xsd:decimal ;
	sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Validation of hatSollwert failed!"@en ;
        sh:name "hatSollwert" ;
        sh:path leakage-test:hatSollwert ] ,
    [ skos:example "0" ;
        sh:datatype xsd:decimal ;
	sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Validation of hatUnterToleranz failed!"@en ;
        sh:name "hatUntereToleranz" ;
        sh:path leakage-test:hatUnterToleranz ] ; 
    sh:targetClass leakage-test:MaximalerDruck.
    
