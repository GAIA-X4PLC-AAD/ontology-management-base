@prefix Geodata_CityGML: <https://github.com/GAIA-X4PLC-AAD/map-and-scenario-data/tools/ontologie_creator/ontologies/Geodata_CityGML_ontology/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

Geodata_CityGML:Asset_Shape a sh:NodeShape ;
    sh:property [ skos:example "Building, Bridge, Vegetation, .. " ;
            sh:datatype xsd:string ;
            sh:description "Included CityGML top-level feature types"@en ;
            sh:message "Validation of featureTypes failed!"@en ;
            sh:minCount "0"^^xsd:unsignedInt ;
            sh:name "featureTypes"@en ;
            sh:path Geodata_CityGML:Geodata_CityGML_quality_featureTypes ],
        [ skos:example "2011-11-11 00:00:00" ;
            sh:datatype xsd:string ;
            sh:description "Creation Date"@en ;
            sh:message "Validation of creationDate failed!"@en ;
            sh:minCount "0"^^xsd:unsignedInt ;
            sh:name "creationDate"@en ;
            sh:path Geodata_CityGML:Geodata_CityGML_quality_creationDate ],
        [ skos:example "LDBV Commercial" ;
            sh:datatype xsd:anyURI ;
            sh:description "License"@en ;
            sh:message "Validation of license failed!"@en ;
            sh:minCount "0"^^xsd:unsignedInt ;
            sh:name "license"@en ;
            sh:path Geodata_CityGML:Geodata_CityGML_quality_license ],
        [ skos:example "0,1,2,3,4" ;
            sh:datatype xsd:int ;
            sh:description "Level of detail according to cityGML Standard"@en ;
            sh:message "Validation of lod failed!"@en ;
            sh:minCount "0"^^xsd:unsignedInt ;
            sh:name "lod"@en ;
            sh:path Geodata_CityGML:Geodata_CityGML_quality_lod ],
        [ skos:example "2025-12-31 00:00:00" ;
            sh:datatype xsd:string ;
            sh:description "TerminationDate"@en ;
            sh:message "Validation of terminationDate failed!"@en ;
            sh:minCount "0"^^xsd:unsignedInt ;
            sh:name "terminationDate"@en ;
            sh:path Geodata_CityGML:Geodata_CityGML_quality_terminationDate ],
        [ skos:example "2" ;
            sh:datatype xsd:int ;
            sh:description "CityGML Version"@en ;
            sh:message "Validation of version failed!"@en ;
            sh:minCount "0"^^xsd:unsignedInt ;
            sh:name "version"@en ;
            sh:path Geodata_CityGML:Geodata_CityGML_quality_version ],
        [ skos:example "1221211" ;
            sh:datatype xsd:string ;
            sh:description "Number of features"@en ;
            sh:message "Validation of numberOfFeatures failed!"@en ;
            sh:minCount "0"^^xsd:unsignedInt ;
            sh:name "numberOfFeatures"@en ;
            sh:path Geodata_CityGML:Geodata_CityGML_quality_numberOfFeatures ] ;
    sh:targetClass Geodata_CityGML:Asset .

