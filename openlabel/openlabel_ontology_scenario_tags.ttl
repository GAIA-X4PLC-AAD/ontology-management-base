    rdfs:label "Scenario parent reference"@en ;
    rdfs:comment "This is a universally unique identifier (UUID) which identifies the scenario which this one has been derived from."@en ;
    rdfs:domain <Scenario> ;
    rdfs:range rdfs:Literal .

<scenarioName> a rdfs:Property ;
    rdfs:label "Scenario name"@en ;
    rdfs:comment "The name of the scenario"@en ;
    rdfs:domain <Scenario> ;
    rdfs:range rdfs:Literal .

<scenarioDescription> a rdfs:Property ;
    rdfs:label "Scenario description"@en ;
    rdfs:comment "A description of the scenario"@en ;
    rdfs:domain <Scenario> ;
    rdfs:range rdfs:Literal .

<scenarioVersion> a rdfs:Property ;
    rdfs:label "Scenario version"@en ;
    rdfs:comment "The version number of the scenario"@en ;
    rdfs:domain <Scenario> ;
    rdfs:range rdfs:Literal .

<scenarioCreatedDate> a rdfs:Property ;
