@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix surveyonto: <http://semanticweb.org/metadatasurveyontology/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

surveyonto:SurveyServiceOfferingShape
	a sh:NodeShape ; 
	sh:targetClass surveyonto:SurveyServiceOffering ; 
	sh:property [
		sh:path surveyonto:survey_id ; 
		sh:datatype xsd:string ; 
		skos:example "'Example ID'" ;
		sh:minCount 1 ; 
		sh:message "Missing survey ID." ;
		sh:name "Survey ID" ;
	] ; 
	sh:property [
		sh:path surveyonto:survey_title ; 
		sh:datatype xsd:string ; 
		skos:example "'Example Title of Survey'" ;
		sh:minCount 1 ;
		sh:message "Missing Survey Title." ;
		sh:name "Survey title" ;
	] ; 
	sh:property [
		sh:path surveyonto:survey_description ; 
		sh:datatype xsd:string ; 
		skos:example "'Example Description of Survey'" ; 
		sh:minCount 1 ;
		sh:message "Missing survey description." ;
		sh:name "Survey description" ;
	] ;
	sh:property [
		sh:path surveyonto:survey_url ; 
		sh:datatype xsd:string ; 
		skos:example "'Example URL'" ; 
		sh:minCount 1 ;
		sh:message "Missing survey URL." ;
		sh:name "Survey URL" ;
	] ; 
	sh:property [
		sh:path surveyonto:survey_creation_time ; 
		sh:datatype xsd:dateTime ;  
		skos:example "'Date 1'" ; 
		sh:minCount 1 ; 
		sh:message "Missing survey creation time." ;
		sh:name "Survey creation time" ;
		sh:description "When the survey was created" ;
	] ; 
		sh:property [
		sh:path surveyonto:survey_creator ; 
		sh:datatype xsd:string ;  
		skos:example "'Example Creator'" ; 
		sh:minCount 1 ; 
		sh:message "Missing survey creator." ;
		sh:name "Survey creator" ;
		sh:description "Who created the survey" ;
	] ; 
	sh:property [
		sh:path surveyonto:survey_end_time ; 
		sh:datatype xsd:dateTime ; 
		skos:example "'Date 3'" ; 
		sh:minCount 1 ;
		sh:message "Missing survey end time." ;
		sh:name "Survey end time" ;
		sh:description "When the survey will end automatically" ;
	] .