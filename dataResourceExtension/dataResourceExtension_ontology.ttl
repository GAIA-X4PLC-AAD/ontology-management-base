@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix gax-core: <https://w3id.org/gaia-x/core#> .
@prefix dataResourceExtension: <https://github.com/GAIA-X4PLC-AAD/ontology-management-base/tree/main/dataResourceExtension/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix gx: <https://registry.lab.gaia-x.eu/development/api/trusted-shape-registry/v1/shapes/jsonld/trustframework#> .

dataResourceExtension: a owl:Ontology ;
    rdfs:label "ontology definition for DataResourceExtension"@en ;
    dcterms:contributor "Mirco Nierenz (TG)" ;
    owl:versionInfo "0.1" .

dataResourceExtension:DataResourceExtension a owl:Class ;
    rdfs:label "Class definition for DataResourceExtension" ;
    rdfs:comment "DataResourceExtension attributes for all assets"@en ;
    rdfs:subClassOf gx:DataResource .

dataResourceExtension:BundleData a owl:Class ;
    rdfs:label "BundleData definition for DataResourceExtension" ;
    rdfs:comment "BundleData attributes for all assets"@en .

dataResourceExtension:Links a owl:Class ;
    rdfs:label "Links definition for DataResourceExtension" ;
    rdfs:comment "Links attributes for all assets"@en .

dataResourceExtension:Link a owl:Class ;
    rdfs:label "Link definition for DataResourceExtension" ;
    rdfs:comment "Link attributes for all assets"@en .

