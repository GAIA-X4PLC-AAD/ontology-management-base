@prefix leakage-test: <https://w3id.org/gaia-x4plcaad/ontologies/leakage-test/v2/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix gx: <https://w3id.org/gaia-x/development#> .
@prefix general: <https://w3id.org/gaia-x4plcaad/ontologies/general/v3/> .

<https://w3id.org/gaia-x4plcaad/ontologies/leakage-test/v2> a owl:Ontology ;
	rdfs:label "Ontology for leakage-test"@en ;

	dcterms:creator "The GAIA-X 4 PLC AAD Project Team"@en ;
    dcterms:contributor [
        a foaf:Person ;
        foaf:name "Selcan Karadag" ;
        org:memberOf <https://www.continental.com> ;
    ] ;  

    # Permanent identifier for the ontology (not version-specific)
    dcterms:identifier <https://w3id.org/gaia-x4plcaad/ontologies/leakage-test> ;

    # GitHub repository where the ontology is maintained
    dcterms:source <https://github.com/GAIA-X4PLC-AAD/ontology-management-base> ;

    # Specific GitHub release used for publishing this version
    prov:wasDerivedFrom <https://github.com/GAIA-X4PLC-AAD/ontology-management-base/releases/tag/v0.0.5> ;

    # Versioning
    owl:versionIRI <https://w3id.org/gaia-x4plcaad/ontologies/leakage-test/v2/> ;
    owl:versionInfo "v2" ;

    # Reference to SHACL validation rules
    dcterms:conformsTo <https://w3id.org/gaia-x4plcaad/ontologies/leakage-test/v2/shapes> ;

    # Additional references to documentation or related ontologies
    dcterms:references <https://w3id.org/gaia-x/development#> ;
    rdfs:seeAlso <https://github.com/GAIA-X4PLC-AAD/ontology-management-base/blob/main/README.md> .

# Classes

leakage-test:LeakageTest a owl:Class ;
	rdfs:label "Class definition for LeakageTest"@en ;
	rdfs:comment "Describes a leakage test."@en ;
	rdfs:subClassOf gx:SoftwareResource .

leakage-test:MachineParameter a owl:Class ;
	rdfs:label "Class definition for MachineParameter"@en ;
	rdfs:comment "Attributes for machine parameter of a leakage test."@en .

leakage-test:ProcessParameter a owl:Class ;
	rdfs:label "Class definition for ProcessParameter"@en ;
	rdfs:comment "Attributes for process parameter of a leakage test."@en .

leakage-test:CodingDownholder a owl:Class ;
	rdfs:label "Class definition for CodingDownholder"@en ;
	rdfs:comment "Attributes for coding downholder of a leakage test."@en .

leakage-test:CodingTestAdapter a owl:Class ;
	rdfs:label "Class definition for CodingTestAdapter"@en ;
	rdfs:comment "Attributes for coding test adapter of a leakage test."@en .

leakage-test:CodingRFID a owl:Class ;
	rdfs:label "Class definition for CodingRFID"@en ;
	rdfs:comment "Attributes for coding RFID of a leakage test."@en .

leakage-test:MaximumPressure a owl:Class ;
	rdfs:label "Class definition for MaximumPressure"@en ;
	rdfs:comment "Attributes for maximum pressure of a leakage test."@en .

leakage-test:MinimumPressure a owl:Class ;
	rdfs:label "Class definition for MinimumPressure"@en ;
	rdfs:comment "Attributes for minimum pressure of a leakage test."@en .

# Properties

leakage-test:general a owl:ObjectProperty ;
    rdfs:label "general"@en ;
    rdfs:comment "General information about the leakage test."@en ;
    rdfs:domain leakage-test:LeakageTest ;
    rdfs:range general:General .

leakage-test:machineParameter a owl:ObjectProperty ;
    rdfs:label "machine parameter"@en ;
    rdfs:comment "Machine parameters for the test."@en ;
    rdfs:domain leakage-test:LeakageTest ;
    rdfs:range leakage-test:MachineParameter .

leakage-test:processParameter a owl:ObjectProperty ;
    rdfs:label "process parameter"@en ;
    rdfs:comment "Process parameters for the test."@en ;
    rdfs:domain leakage-test:LeakageTest ;
    rdfs:range leakage-test:ProcessParameter .

leakage-test:codingRFID a owl:ObjectProperty ;
    rdfs:label "coding RFID"@en ;
    rdfs:domain leakage-test:ProcessParameter ;
    rdfs:range leakage-test:CodingRFID .

leakage-test:codingTestAdapter a owl:ObjectProperty ;
    rdfs:label "coding test adapter"@en ;
    rdfs:domain leakage-test:ProcessParameter ;
    rdfs:range leakage-test:CodingTestAdapter .

leakage-test:codingDownholder a owl:ObjectProperty ;
    rdfs:label "coding downholder"@en ;
    rdfs:domain leakage-test:ProcessParameter ;
    rdfs:range leakage-test:CodingDownholder .

leakage-test:maximumPressure a owl:ObjectProperty ;
    rdfs:label "maximum pressure"@en ;
    rdfs:domain leakage-test:MachineParameter ;
    rdfs:range leakage-test:MaximumPressure .

leakage-test:minimumPressure a owl:ObjectProperty ;
    rdfs:label "minimum pressure"@en ;
    rdfs:domain leakage-test:MachineParameter ;
    rdfs:range leakage-test:MinimumPressure .

leakage-test:setpoint a owl:DatatypeProperty ;
    rdfs:label "setpoint"@en ;
    rdfs:comment "Target value."@en ;
    rdfs:range xsd:float . 

leakage-test:lowerTolerance a owl:DatatypeProperty ;
    rdfs:label "lower tolerance"@en ;
    rdfs:range xsd:float .

leakage-test:upperTolerance a owl:DatatypeProperty ;
    rdfs:label "upper tolerance"@en ;
    rdfs:range xsd:float .

leakage-test:unit a owl:DatatypeProperty ;
    rdfs:label "unit"@en ;
    rdfs:range xsd:string .
