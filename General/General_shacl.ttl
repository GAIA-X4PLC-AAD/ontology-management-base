@prefix General: <https://github.com/GAIA-X4PLC-AAD/map-and-scenario-data/General_ontology> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

General:Asset_Shape a sh:NodeShape ;
    sh:property [ skos:example "3D Model in Grafing" ;
            sh:datatype xsd:string ;
            sh:description "name of data asset"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of name failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "name"@en ;
            sh:path General:General_description_name ],
        [ skos:example "road network, road surface, scenario, sensor model, environment model" ;
            sh:datatype xsd:string ;
            sh:description "short data type description"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of type failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "type"@en ;
            sh:path General:General_description_type ],
        [ skos:example "Lat: 1.234- 1.345; Lon 45.23-45.34" ;
            sh:datatype xsd:float ;
            sh:description "bounding box with lat/lon values in WGS84"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of bounding failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "bounding"@en ;
            sh:path General:General_position_bounding ],
        [ skos:example "2022-10-01 00:00:00" ;
            sh:datatype xsd:string ;
            sh:description "creation date"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of date failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "date"@en ;
            sh:path General:General_vendor_date ],
        [ skos:example "Germany, GER" ;
            sh:datatype xsd:string ;
            sh:description "ISO country code, ISO 3166-1, alpha-2; "@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of country failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "country"@en ;
            sh:path General:General_position_country ],
        [ skos:example "link to licence" ;
            sh:datatype xsd:anyURI ;
            sh:description "reference to licence"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of licence_link failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "licence_link"@en ;
            sh:path General:General_licence_licence_link ],
        [ skos:example "1000" ;
            sh:datatype xsd:float ;
            sh:description "in euros"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of price failed!"@en ;
            sh:minCount "0"^^xsd:unsignedInt ;
            sh:name "price"@en ;
            sh:path General:General_price_price ],
        [ skos:example "TrianGraphics GmbH" ;
            sh:datatype xsd:string ;
            sh:description "name as registered in data space"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of name failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "name"@en ;
            sh:path General:General_vendor_name ],
        [ skos:example "OpenDRIVE / Road5 / Here Live Map / Lanelet / DLM / road2sim / roadXML  / Shape" ;
            sh:datatype xsd:string ;
            sh:description "format dedinition"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of data_format failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "data_format"@en ;
            sh:path General:General_format_data_format ],
        [ skos:example "link to screenshot, video, routing" ;
            sh:datatype xsd:anyURI ;
            sh:description "reference to screenshot data"@en ;
            sh:message "Validation of media_link failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "media_link"@en ;
            sh:path General:General_links_media_link ],
        [ skos:example "1.5" ;
            sh:datatype xsd:string ;
            sh:description "nan"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of format_version failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "format_version"@en ;
            sh:path General:General_format_format_version ],
        [ skos:example "town district, with traffic signs" ;
            sh:datatype xsd:string ;
            sh:description "to which category to be classified, what special characteristics, features"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of description_by_vendor failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "description_by_vendor"@en ;
            sh:path General:General_description_description_by_vendor ],
        [ skos:example "MIT" ;
            sh:datatype xsd:string ;
            sh:description "name of licence type"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of licence_type failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "licence_type"@en ;
            sh:path General:General_licence_licence_type ],
        [ skos:example "link to CRG" ;
            sh:datatype xsd:anyURI ;
            sh:description "reference to dependent data"@en ;
            sh:message "Validation of data_link failed!"@en ;
            sh:minCount "0"^^xsd:unsignedInt ;
            sh:name "data_link"@en ;
            sh:path General:General_links_data_link ] ;
    sh:targetClass General:Asset .

