@prefix sen-dat: <http://www.ipk-semantics.org/konierik/ontologies/SensorDataMetadata#> .
@prefix schema: <https://schema.org/>.
@prefix plcaad-tbd: <http://www.WirSchauenNachPassendenVokabeln.org/platzhalter#>.
@prefix gax-tbd: <http://www.BestimmtGibtsHierzuEineGAXKlasse.org/findeSie#>.
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix gax-core: <https://w3id.org/gaia-x/core#> .
@prefix dc: <http://purl.org/dc/terms/>.


<http://www.ipk-semantics.org/konierik/ontologies/SensorDataMetadataShape> rdf:type owl:Ontology ;
dc:created "2024-03-04";                                             
dc:creator "Erik Konietzko "@konierik.

sen-dat:SensorDataShape 
	a sh:NodeShape ;
	sh:targetClass sen-dat:SensorData ;
	
#######################################
###
###		SensorData DatatypeProperties
###
#######################################

	sh:property
	[
		sh:path sen-dat:timeReferenceFrame ;
		sh:datatype xsd:string;
		skos:example "???" ; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add exactly one timestamp";
		sh:name "Reference Frame" ; 
		sh:description "Internal timestamp relative to start or absolute."@en ; 
	] ; 
	sh:property
	[
		sh:path sen-dat:timeDuration ;
		sh:datatype xsd:float;
		skos:example "12.0" ; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add exactly one duration period";
		sh:name "Duration (in ms)" ; 
		sh:description "Duration of sensor data trace (ms)"@en ; 
	] ; 
	sh:property
	[
		sh:path sen-dat:timeStartTime ;
		sh:datatype xsd:string; ### We need to check if ISO8601 time format is available
		skos:example "2023-06-19T15-24-30.400491149Z" ; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add exactly one start time";
		sh:name "Start time" ; 
		sh:description "Timestamp of first frame incl. timezone (ISO 8601)"@en ; 
	] ; 
	sh:property
	[
		sh:path sen-dat:toolChain ;
		sh:datatype xsd:string; ### We need to check if ISO8601 time format is available
		skos:example "Captured by Tool X as rosbag" ; 
		sh:minCount 1 ;
		###sh:maxCount 1 ; 
		sh:message "Add min one tool chain element";
		sh:name "Tool Chain" ; 
		sh:description "Listings of tools, that where used to create and modify the sensor data"@en ; 
	] ; 


#########################################
###
###		SensorData Objectproperties
###
#########################################

	### SensorData has exactly 1 Author
	sh:property[ 
		sh:path schema:author;
		sh:node sen-dat:AuthorShape;
		sh:minCount 1;
		sh:maxCount 1;
		sh:name "Author of sensordata";
		sh:description "A person, that took part in the creation of sensordata" @en;
		sh:message "You need to add exactly one IRI of an author." @en;
		skos:example "prefix:AuthorIRI";		
	];	
	
	### SensorData has exactly 1 SensorDataFile
	sh:property[ 
		sh:path sen-dat:sensorDataFile;
		sh:node sen-dat:SensorDataFileShape;
		sh:minCount 1;
		sh:maxCount 1;
		sh:name "Sensor Data File";
		sh:description "File containing the sensordata" @en;
		sh:message "Add a reference to one sensor data file" @en;
		skos:example "prefix:SensorDataFileIRI";
	];
	
	### SensorData has exactly 1 Localization
	sh:property[ 
		sh:path sen-dat:inLocalization;
		sh:node sen-dat:SRMD_LocalizationShape;
		sh:minCount 1;
		sh:maxCount 1;
		sh:name "SRMD Localization";
		sh:description "Reference to a localization individual where the sensor data was captured" @en;
		sh:message "Add a reference to one Localization object" @en;
		skos:example "prefix:LocalizationIRI";
	];
	
	### SensorData has exactly 1 Environment
	sh:property[ 
		sh:path sen-dat:inEnvironment;
		sh:node sen-dat:SRMD_EnvironmentShape;
		sh:minCount 1;
		sh:maxCount 1;
		sh:name " SRMD Environment";
		sh:description "Reference to an environment individual where the sensor data was captured" @en;
		sh:message "Add a reference to one Environment object" @en;
		skos:example "prefix:EnvironmentIRI";
	];
	
	### SensorData is produced by exactly 1 Sensor
	sh:property[ 
		sh:path sen-dat:producedBySensor;
		sh:node sen-dat:SensorShape;
		sh:minCount 1;
		sh:maxCount 1;
		sh:name "Sensor";
		sh:description "Reference to a sensor individual that created the data" @en;
		sh:message "Add a reference to one Sensor object" @en;
		skos:example "prefix:SensorIRI";
	].	### close SensorDataShape
	
	
########################################
###
###		other class restrictions --> All of them should be in individual shapes. But one file is easier for testing purposes :)
###
########################################

###Author
sen-dat:AuthorShape
	a sh:NodeShape;
	sh:targetClass sen-dat:Author;
	sh:or(
		[
			sh:property[
				sh:path schema:telephone;
				sh:datatype xsd:string;
				skos:example "<+49 1234 456789>"; 
				sh:minCount 1 ;
				###sh:maxCount 1 ; 
				sh:message "Add at least one contact detail (Email or telephone).";
				sh:name "Author Contact" ; 
				sh:description "Contact for responsible party"@en ; 
			]
		]
		[
			sh:property[
				sh:path schema:email;
				sh:datatype xsd:string;
				skos:example "<example@examplecompany.com>"; 
				sh:minCount 1 ;
				###sh:maxCount 1 ; 
				sh:message "Add at least one contact detail. (Email or telephone)";
				sh:name "Author Contact" ; 
				sh:description "Contact for responsible party"@en ; 
			]
		]
	);
	sh:property[
		sh:path schema:memberOf ;
		sh:node sen-dat:OrganizationShape;
		skos:example "Umbrella" ; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add the company IRI the author was working for during creation of the dataset" @en;
		sh:name "Author Company" ; 
		sh:description "Company responsible for sensor data trace"@en ; 
	];
	sh:property[
		sh:path schema:givenName ;
		sh:datatype xsd:string;
		skos:example "Peter" ; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add the first name of the author" @en;
		sh:name "Author fist name" ; 
		sh:description "First Name of author"@en ; 
	];
	sh:property[
		sh:path schema:familyName ;
		sh:datatype xsd:string;
		skos:example "Peterson" ; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add the family name of the author" @en;
		sh:name "Author family name" ; 
		sh:description "Family Name of author"@en ; 
	].
	
###SensorDataFile
sen-dat:SensorDataFileShape
	a sh:NodeShape;
	sh:targetClass sen-dat:SensorDataFile;
	sh:property[
		sh:path sen-dat:fileName;
		sh:datatype xsd:string;
		skos:example "20230619T150000Z_sd_312_3200_1256_AVL_ars548_054806_cut-out"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add at exactly one file name.";
		sh:name "Sensor Data File Name" ; 
		sh:description "Name of file containing referenced sensordata"@en ; 
	];
	sh:property[
		sh:path sen-dat:fileSize;
		sh:datatype xsd:int;
		skos:example "700 KB"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add file size in KB.";
		sh:name "Sensor Data File Size (in KB)" ; 
		sh:description "Size of file containing referenced sensordata"@en ; 
	];
	sh:property[
		sh:path sen-dat:checksum; 
		sh:datatype xsd:string;
		skos:example "???"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add exactly one file checksum.";
		sh:name "Sensor Data File Checksum" ; 
		sh:description "???"@en ; 
	];
	sh:property[
		sh:path sen-dat:mimetype;
		sh:datatype xsd:string;
		skos:example "osi, pcapng, ..."; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add exactly one file mimetype.";
		sh:name "Sensor Data File Mimetype" ; 
		sh:description "Mimetype of sensor data file"@en ; 
	].
	
###Sensor
