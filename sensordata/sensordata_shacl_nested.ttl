@prefix sen-dat: <http://www.ipk-semantics.org/konierik/ontologies/SensorDataMetadata#> .
@prefix schema: <https://schema.org/>.
@prefix plcaad-tbd: <http://www.WirSchauenNachPassendenVokabeln.org/platzhalter#>.
@prefix gax-tbd: <http://www.BestimmtGibtsHierzuEineGAXKlasse.org/findeSie#>.
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix gax-core: <https://w3id.org/gaia-x/core#> .
@prefix dc: <http://purl.org/dc/terms/>.


<http://www.ipk-semantics.org/konierik/ontologies/SensorDataMetadataShape> rdf:type owl:Ontology ;
dc:created "2024-03-04";                                             
dc:creator "Erik Konietzko "@konierik.

sen-dat:SensorDataShape 
	a sh:NodeShape ;
	sh:targetClass sen-dat:SensorData ;
	
#######################################
###
###		SensorData DatatypeProperties
###
#######################################

	sh:property
	[
		sh:path sen-dat:timeReferenceFrame ;
		sh:datatype xsd:string;
		skos:example "???" ; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Optional: Add a timestamp";
		sh:name "Reference Frame" ; 
		sh:description "Internal timestamp relative to start or absolute."@en ; 
	] ; 
	sh:property
	[
		sh:path sen-dat:timeDuration ;
		sh:datatype xsd:float;
		skos:example "12.0" ; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Optional: Add a duration period";
		sh:name "Duration (in ms)" ; 
		sh:description "Duration of sensor data trace (ms)"@en ; 
	] ; 
	sh:property
	[
		sh:path sen-dat:timeStartTime ;
		sh:datatype xsd:string; ### We need to check if ISO8601 time format is available
		skos:example "2023-06-19T15-24-30.400491149Z" ; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Optional: Add a start time";
		sh:name "Start time" ; 
		sh:description "Timestamp of first frame incl. timezone (ISO 8601)"@en ; 
	] ; 


#########################################
###
###		SensorData Objectproperties
###
#########################################

	### SensorData has exactly 1 Author
	sh:property[ 
		sh:path schema:author;
		sh:node sen-dat:AuthorShape;
		sh:minCount 1;
		sh:maxCount 1;
		sh:name "Author of sensordata";
		sh:description "A person, that took part in the creation of sensordata" @en;
		sh:message "You need to add exactly one IRI of an author." @en;
		skos:example "prefix:AuthorIRI";		
	];	
	
	### SensorData has exactly 1 Localization
	sh:property[ 
		sh:path sen-dat:inLocalization;
		sh:node sen-dat:SRMD_LocalizationShape;
		sh:minCount 1;
		sh:maxCount 1;
		sh:name "SRMD Localization";
		sh:description "Reference to a localization individual where the sensor data was captured" @en;
		sh:message "Add a reference to one Localization object" @en;
		skos:example "prefix:LocalizationIRI";
	];
	
	### SensorData has exactly 1 Environment
	sh:property[ 
		sh:path sen-dat:inEnvironment;
		sh:node sen-dat:SRMD_EnvironmentShape;
		sh:minCount 1;
		sh:maxCount 1;
		sh:name " SRMD Environment";
		sh:description "Reference to an environment individual where the sensor data was captured" @en;
		sh:message "Add a reference to one Environment object" @en;
		skos:example "prefix:EnvironmentIRI";
	];
	
	### SensorData is produced by exactly 1 Sensor
	sh:property[ 
		sh:path sen-dat:producedBySensor;
		sh:node sen-dat:SensorShape;
		sh:minCount 1;
		sh:maxCount 1;
		sh:name "Sensor";
		sh:description "Reference to a sensor individual that created the data" @en;
		sh:message "Add a reference to one Sensor object" @en;
		skos:example "prefix:SensorIRI";
	];
	### SensorData refers to min 0 Toolchain
	sh:property[ 
		sh:path sen-dat:usedToolchain;
		sh:node sen-dat:ToolchainShape;
		sh:minCount 0;
		sh:maxCount 1;
		sh:name "Toolchain";
		sh:description "Used toolchain to process the sensor data" @en;
		sh:message "Optional: Add a reference to a toolchain object" @en;
		skos:example "prefix:ToolchainIRI";
	];
	### SensorData refers to min 0 Testsetup
	sh:property[ 
		sh:path sen-dat:sensorTestsetup;
		sh:node sen-dat:TestsetupShape;
		sh:minCount 0;
		sh:maxCount 1;
		sh:name "Testsetup";
		sh:description "Used testsetup for sensor data capture" @en;
		sh:message "Optional: Add a reference to a testsetup object" @en;
		skos:example "prefix:TestsetupIRI";
	];	
	### SensorData refers to min 0 Content
	sh:property[ 
		sh:path sen-dat:sensorDataContent;
		sh:node sen-dat:ContentShape;
		sh:minCount 0;
		sh:maxCount 1;
		sh:name "Sensordata Content";
		sh:description "Content of sensor data " @en;
		sh:message "Optional: Add a reference to a content object" @en;
		skos:example "prefix:ContentIRI";
	];	
	
#########################################
###
###		SensorData Document links
###
#########################################
	
	### SensorDataFile link
	sh:or(
		[
			### either document link to a Document object
			### SensorData has  SensorDataFile
			sh:property[ 
				sh:path sen-dat:sensorDataDocumentLink;
				sh:node sen-dat:SensorDataDocumentShape;
				sh:minCount 0;
				sh:maxCount 1;
				sh:name "Sensor Data File";
				sh:description "File containing the sensordata" @en;
				sh:message "Optional: Add a reference to one sensor data file object" @en;
				skos:example "prefix:SensorDataFileIRI";
			]
		]
		[
			### or fill attribute modelLink with link to document
			### SensorData has modellink
			sh:property[ 
				sh:path sen-dat:modelLink;
				sh:or(
					[
						sh:datatype xsd:string;
					]
					[
						sh:datatype xsd:anyURI;
					]
				);
				sh:minCount 0;
				sh:maxCount 1;
				sh:name "Link to Sensor Data File";
				sh:description "link to sensordata" @en;
				sh:message "Optional: Add a link to a sensor data file " @en;
				skos:example "https://www.linkToElsewhere.org";
			]
		]	
	);
	sh:message "Optional: Add either modelLink attribute or refer to a Sensor Data Document Object";
	
	###gpsTraceDocumentLink
	sh:or(
		[
			### either document link to a Document object
			### SensorData has  GPSTraceDocument
			sh:property[ 
				sh:path sen-dat:gpsTraceDocumentLink;
				sh:node sen-dat:DocumentShape;
				sh:minCount 0;
				sh:maxCount 1;
				sh:name "GPS trace file";
				sh:description "link to gps trace file" @en;
				sh:message "Optional: Add a reference to a GPSTraceDocument object" @en;
				skos:example "prefix:GPSTraceDocumentIRI";
			]
		]
		[
			### or fill attribute modelLink with link to document
			### SensorData has modellink
			sh:property[ 
				sh:path sen-dat:modelLink;
				sh:or(
					[
						sh:datatype xsd:string;
					]
					[
						sh:datatype xsd:anyURI;
					]
				);
				sh:minCount 0;
				sh:maxCount 1;
				sh:name "Link to gps trace file";
				sh:description "link to gps trace file" @en;
				sh:message "Optional: Add a link to a gps trace file " @en;
				skos:example "https://www.linkToElsewhere.org";
			]
		]	
	);
	sh:message "Optional: Add either modelLink attribute or refer to a GPS Trace Document Object";

	###SensorTraceDocumentLink
	sh:or(
		[
			### either document link to a Document object
			### SensorData has  SensorTraceDocument
			sh:property[ 
				sh:path sen-dat:sensorTraceDocumentLink;
				sh:node sen-dat:DocumentShape;
				sh:minCount 0;
				sh:maxCount 1;
				sh:name "sensor trace file";
				sh:description "link to sensor trace file" @en;
				sh:message "Optional: Add a reference to a SensorTraceDocument object" @en;
				skos:example "prefix:SensorTraceDocumentIRI";
			]
		]
		[
			### or fill attribute modelLink with link to document
			### SensorData has modellink
			sh:property[ 
				sh:path sen-dat:modelLink;
				sh:or(
					[
						sh:datatype xsd:string;
					]
					[
						sh:datatype xsd:anyURI;
					]
				);
				sh:minCount 0;
				sh:maxCount 1;
				sh:name "Link to sensor trace file";
				sh:description "link to sensor trace file" @en;
				sh:message "Optional: Add a link to a sensor trace file " @en;
				skos:example "https://www.linkToElsewhere.org";
			]
		]	
	);
	sh:message "Optional: Add either modelLink attribute or refer to a Sensor Trace Document Object";	
	
	###sensorDataRawDocumentLink
	sh:or(
		[
			### either document link to a Document object
			### SensorData has  SensorDataRawDocument
			sh:property[ 
				sh:path sen-dat:sensorDataRawDocumentLink;
				sh:node sen-dat:DocumentShape;
				sh:minCount 0;
				sh:maxCount 1;
				sh:name "file of sensor raw data capture";
				sh:description "link to raw data file" @en;
				sh:message "Optional: Add a reference to a SensorRawDataDocument object" @en;
				skos:example "prefix:SensorRawDocumentIRI";
			]
		]
		[
			### or fill attribute modelLink with link to document
			### SensorData has modelLink
			sh:property[ 
				sh:path sen-dat:modelLink;
				sh:or(
					[
						sh:datatype xsd:string;
					]
					[
						sh:datatype xsd:anyURI;
					]
				);
				sh:minCount 0;
				sh:maxCount 1;
				sh:name "Link to raw data file";
				sh:description "link to raw data file" @en;
				sh:message "Optional: Add a link to a raw data file " @en;
				skos:example "https://www.linkToElsewhere.org";
			]
		]	
	);
	sh:message "Optional: Add either modelLink attribute or refer to a Sensor Raw Data Document Object";

	###groundTruthDocumentLink
	sh:or(
		[
			### either document link to a Document object
			### SensorData has  GroundTruthDocument
			sh:property[ 
				sh:path sen-dat:groundTruthDocumentLink;
				sh:node sen-dat:DocumentShape;
				sh:minCount 0;
				sh:maxCount 1;
				sh:name "Ground Truth Data";
				sh:description "Link to Ground Truth Data Object" @en;
				sh:message "Optional: Add a reference to a GroundTruthDocument object" @en;
				skos:example "prefix:GroundTruthDocumentIRI";
			]
		]
		[
			### or fill attribute modelLink with link to document
			### SensorData has modelLink
			sh:property[ 
				sh:path sen-dat:modelLink;
				sh:or(
					[
						sh:datatype xsd:string;
					]
					[
						sh:datatype xsd:anyURI;
					]
				);
				sh:minCount 0;
				sh:maxCount 1;
				sh:name "Ground truth data file";
				sh:description "link to ground truth data file" @en;
				sh:message "Optional: Add a link to a ground truth data file " @en;
				skos:example "https://www.linkToElsewhere.org";
			]
		]	
	);
	sh:message "Optional: Add either modelLink attribute or refer to a Ground Truth Document Object"
	.	### close SensorDataShape
	
	
########################################
###
###		other class restrictions --> All of them should be in individual shapes. But one file is easier for testing purposes :)
###
########################################

###Author
sen-dat:AuthorShape
	a sh:NodeShape;
	sh:targetClass sen-dat:Author;
	### add either email or telephone number of author
	sh:or(
		[
			### telephone number of author
			sh:property[
				sh:path schema:telephone;
				sh:datatype xsd:string;
				skos:example "<+49 1234 456789>"; 
				sh:minCount 1 ;
				###sh:maxCount 1 ; 
				sh:message "Add at least one contact detail (Email or telephone).";
				sh:name "Author Contact" ; 
				sh:description "Contact for responsible party"@en ; 
			]
		]
		[
			### email of author
			sh:property[
				sh:path schema:email;
				sh:datatype xsd:string;
				skos:example "<example@examplecompany.com>"; 
				sh:minCount 1 ;
				###sh:maxCount 1 ; 
				sh:message "Add at least one contact detail. (Email or telephone)";
				sh:name "Author Contact" ; 
				sh:description "Contact for responsible party"@en ; 
			]
		]
	);
	###given name of author
	sh:property[
		sh:path schema:givenName ;
		sh:datatype xsd:string;
		skos:example "Peter" ; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Optional: Add the first name of the author" @en;
		sh:name "Author fist name" ; 
		sh:description "First Name of author"@en ; 
	];
	###family name of author
	sh:property[
		sh:path schema:familyName ;
		sh:datatype xsd:string;
		skos:example "Peterson" ; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Optional: Add the family name of the author" @en;
		sh:name "Author family name" ; 
		sh:description "Family Name of author"@en ; 
	];
	
	###Author Object Properties
	
	###Author is member of organisation
	sh:property[
		sh:path schema:memberOf ;
		sh:node sen-dat:OrganizationShape;
		skos:example "Umbrella" ; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Optional: Add the company IRI the author was working for during creation of the dataset" @en;
		sh:name "Author Company" ; 
		sh:description "Company responsible for sensor data trace"@en ; 
	].
	
###SensorDataDocument
sen-dat:SensorDataDocumentShape
	a sh:NodeShape;
	sh:targetClass sen-dat:SensorDataDocument;
	sh:property[
		sh:path sen-dat:fileName;
		sh:datatype xsd:string;
		skos:example "20230619T150000Z_sd_312_3200_1256_AVL_ars548_054806_cut-out"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add at exactly one file name.";
		sh:name "Sensor Data File Name" ; 
		sh:description "Name of file containing referenced sensordata"@en ; 
	];
	sh:property[
		sh:path sen-dat:fileSize;
		sh:datatype xsd:int;
		skos:example "700 KB"; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Optional: Add file size in KB.";
		sh:name "Sensor Data File Size (in KB)" ; 
		sh:description "Size of file containing referenced sensordata"@en ; 
	];
	sh:property[
		sh:path sen-dat:checksum; 
		sh:datatype xsd:string;
		skos:example "???"; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Optional: Add a file checksum.";
		sh:name "Sensor Data File Checksum" ; 
		sh:description "???"@en ; 
	];
	sh:property[
		sh:path sen-dat:mimetype;
		sh:datatype xsd:string;
		skos:example "osi, pcapng, ..."; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Optional: Add a file mimetype.";
		sh:name "Sensor Data File Mimetype" ; 
		sh:description "Mimetype of sensor data file"@en ; 
	];
	sh:property[
		sh:path sen-dat:modelLink;
		sh:datatype xsd:string;
		skos:example "https://www.hierLiegtEinDocument.org/document..."; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add a link to a sensor data document.";
		sh:name "Sensor Data Document Link" ; 
		sh:description "link to sensor data file"@en ; 
	].
	
###Sensor
sen-dat:SensorShape
	a sh:NodeShape;
	sh:targetClass sen-dat:Sensor;
	sh:property[
	sh:path sen-dat:sensorType;
		sh:datatype xsd:string;
		skos:example "Radarsensor 77GHz"; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Optional: Add a Sensor type.";
		sh:name "Sensor Type" ; 
		sh:description "Sensor type"@en ; 
	];
	sh:property[
		sh:path sen-dat:sensorName;
		sh:datatype xsd:string;
		skos:example "ARS548"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add at exactly one sensor name.";
		sh:name "Sensor Name" ; 
		sh:description "Name of Sensor"@en ; 
	];
	sh:property[
		sh:path sen-dat:sensorHardwareVersion;
		sh:datatype xsd:string;
		skos:example "ARS540 HW Rev. x.y"; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Optional: Add a sensor hardware version.";
		sh:name "Sensor Hardware Version" ; 
		sh:description "Sensor Hardware Version"@en ; 
	];
	sh:property[
		sh:path sen-dat:sensorFirmwareVersion;
		sh:datatype xsd:string;
		skos:example "05.48.06"; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Optional: Add a firmware version.";
		sh:name "Sensor Firmware Version" ; 
		sh:description "Sensor Firmware Version"@en ; 
	];
	sh:property[
		sh:path sen-dat:manufacturerPartnumber;
		sh:datatype xsd:string;
		skos:example "xx-xx-xx-xx"; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Optional: Add a partnumber.";
		sh:name "Sensor Manufacturer Partnumber" ; 
		sh:description "Manufacturer Partnumber of sensor"@en ; 
	];
	sh:property[
		sh:path sen-dat:sensorTechnology;
		sh:datatype xsd:string;
		skos:example "Radar"; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Optional: Add a sensor technology.";
		sh:name "Sensor Technoloy" ; 
		sh:description "Technology the sensor uses"@en ; 
	];
	sh:property[
		sh:path sen-dat:sensorTechnologyVariant;
		sh:datatype xsd:string;
		skos:example "long-range"; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Optional: Add a technology variant.";
		sh:name "Sensor Technology Variant" ; 
		sh:description "Name sensor technology variant"@en ; 
	];
	sh:property[
		sh:path sen-dat:sensorStatus;
		sh:datatype xsd:string;
		skos:example "Released"; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Optional: Add a sensor status.";
		sh:name "Sensor Status" ; 
		sh:description "Sensor Status"@en ; 
	];	
	sh:property[
		sh:path sen-dat:sensorPurpose;
		sh:datatype xsd:string;
		skos:example "Long Range Radar Sensor w/ Object Detection"; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Optional: Add a sensor purpose.";
		sh:name "Sensor Purpose" ; 
		sh:description "Sensor Purpose"@en ; 
	];
	###Sensor Object Properties
	### Sensor has SensorSpecification
	sh:or(
		[
			### either document link to a Document object
			### SensorData has  SensorSpecificationDocument
			sh:property[ 
				sh:path sen-dat:sensorSpecificationDocumentLink;
				sh:node sen-dat:SensorSpecificationShape;
				sh:minCount 0;
				sh:maxCount 1;
				sh:name "Sensor Specification";
				sh:description "Reference to a sensor specification individual for the sensor" @en;
				sh:message "Optional: Add a reference to one Sensor Specification object" @en;
				skos:example "prefix:SensorSpecificationIRI";
			]
		]
		[
			### or fill attribute modelLink with link to document
			### Sensor has modelLink
			sh:property[ 
				sh:path sen-dat:modelLink;
				sh:or(
					[
						sh:datatype xsd:string;
					]
					[
						sh:datatype xsd:anyURI;
					]
				);
				sh:minCount 0;
				sh:maxCount 1;
				sh:name "Sensor specification file";
				sh:description "link sensor specification file" @en;
				sh:message "Optional: Add a link to a sensor spefication file " @en;
				skos:example "https://www.linkToElsewhere.org";
			]
		]	
	);
	sh:message "Optional: Add either modelLink attribute or refer to a Sensor Specification Document Object";
	### Sensor has SensorMetadata
	sh:or(
		[
			### either document link to a Document object
			### SensorData has  SensorMetadata
			sh:property[ 
				sh:path sen-dat:SensorMetadataDocument;
				sh:node sen-dat:DocumentShape;
				sh:minCount 0;
				sh:maxCount 1;
				sh:name "Sensor Metadata";
				sh:description "Reference to a document object of the sensor metadata" @en;
				sh:message "Optional: Add a reference to one Sensor Metadata object" @en;
				skos:example "prefix:SensorMetadataDocumentIRI";
			]
		]
		[
			### or fill attribute modelLink with link to document
			### Sensor has modelLink
			sh:property[ 
				sh:path sen-dat:modelLink;
				sh:or(
					[
						sh:datatype xsd:string;
					]
					[
						sh:datatype xsd:anyURI;
					]
				);
				sh:minCount 0;
				sh:maxCount 1;
				sh:name "Sensor metadata file";
				sh:description "link sensor metadata file" @en;
				sh:message "Optional: Add a link to a sensor metadata file " @en;
				skos:example "https://www.linkToElsewhere.org";
			]
		]	
	);
	sh:message "Optional: Add either modelLink attribute or refer to a Sensor Metadata Document Object";
	### Sensor has SensorConfiguration
	sh:property[ 
		sh:path sen-dat:hasSensorConfiguration;
		sh:node sen-dat:SensorConfigurationShape;
		sh:minCount 0;
		sh:maxCount 1;
		sh:name "Sensor Configuration";
		sh:description "Reference to a sensor configuration individual for the sensor" @en;
		sh:message "Optional: Add a reference to one Sensor Configuration object" @en;
		skos:example "prefix:SensorConfigurationIRI";
	];
	### Sensor has sensorManufacturer
	sh:property[ 
		sh:path sen-dat:sensorManufacturer;
		sh:node sen-dat:ManufacturerShape;
		sh:minCount 0;
		sh:maxCount 1;
		sh:name "Sensor Manufacturer";
		sh:description "Reference to a manufacturer individual for the sensor" @en;
		sh:message "Optional: Add a reference to one Sensor Manufacturer object" @en;
		skos:example "prefix:SensorManufacturerIRI";
	];
	### Sensor has Sensor family
	sh:property[ 
		sh:path sen-dat:SensorFamily;
		sh:node sen-dat:SensorFamilyShape;
		sh:minCount 0;
		sh:maxCount 1;
		sh:name "Sensor Family";
		sh:description "Reference to a family class for the sensor" @en;
		sh:message "Optional: Add a reference to a Sensor family object" @en;
		skos:example "prefix:SensorFamilyIRI";
	];
	sh:property[ 
		sh:path sen-dat:producesSensorData;
		sh:node sen-dat:SensorDataShape;
		sh:minCount 0;
		###sh:maxCount 1;
		sh:name "Sensor Data";
		sh:description "Reference to a sensor data class for the sensor" @en;
		sh:message "Optional: Add a reference to a Sensor Data object" @en;
		skos:example "prefix:SensorDataIRI";
	]
