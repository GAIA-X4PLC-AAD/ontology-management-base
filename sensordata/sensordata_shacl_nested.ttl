###SRMD Environment
sen-dat:SRMD_EnvironmentShape
	a sh:NodeShape;
	sh:targetClass sen-dat:SRMD_Environment;
	sh:property[
		sh:path sen-dat:environmentArea;
		sh:datatype xsd:string;
		skos:example "urban|country side|proving ground (road regulations/no road regulations)"; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Add exactly one Area.";
		sh:name "Environment Area" ; 
		sh:description "Environment Area"@en ; 
	];
	sh:property[
		sh:path sen-dat:roadType;
		sh:datatype xsd:string;
		skos:example "freeway|highway|urban road|rural road|..."; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Add exactly one road type.";
		sh:name "Road Type" ; 
		sh:description "Road Type"@en ; 
	];
	sh:property[
		sh:path sen-dat:roadCondition;
		sh:datatype xsd:string;
		skos:example "wet | dry | ice | snow"; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Add exactly one road type condition.";
		sh:name "Road Type Condition" ; 
		sh:description "Specification of road conditions during data capture"@en ; 
	];
	sh:property[
		sh:path sen-dat:weatherTemperature;
		sh:datatype xsd:string;
		skos:example "20 °C"; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Add exactly one temperature with units.";
		sh:name "Weather Temperature" ; 
		sh:description "Specification of temperature during data capture"@en ; 
	];
	sh:property[
		sh:path sen-dat:weatherPrecipation;
		sh:datatype xsd:string;
		skos:example "mm/m^2 or classification"; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Add exactly one weather precipation.";
		sh:name "Weather Precipation" ; 
		sh:description "Specification of precipitation conditions during data capture"@en ; 
	];
	### Environment has Environment model
	sh:or(
		[
			### either document link to a Document object
			### Envionment has  Envionment model
			sh:property[ 
				sh:path sen-dat:environmentModel;
				sh:node sen-dat:DocumentShape;
				sh:minCount 0;
				sh:maxCount 1;
				sh:name "Environment Model";
				sh:description "Reference to an environment model of the environment description" @en;
				sh:message "Optional: Add a reference to one Environment Model object" @en;
				skos:example "prefix:EnvironmentModelIRI";
			]
		]
		[
			### or fill attribute modelLink with link to document
			### Environment has modelLink
			sh:property[ 
				sh:path sen-dat:modelLink;
				sh:or(
					[
						sh:datatype xsd:string;
					]
					[
						sh:datatype xsd:anyURI;
					]
				);
				sh:minCount 0;
				sh:maxCount 1;
				sh:name "Environment model ";
				sh:description "link to environment model" @en;
				sh:message "Optional: Add a link to an environment model" @en;
				skos:example "https://www.linkToElsewhere.org";
			]
		]	
	);
	sh:message "Optional: Add either modelLink attribute or refer to an Environment Model Object" 
	].

###  Generic  Document Shape
sen-dat:DocumentShape
	a sh:NodeShape;
	sh:closed false;
	sh:targetClass sen-dat:Document;
	sh:property[
		sh:path sen-dat:fileName;
		sh:datatype xsd:string;
		skos:example "Document Name"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add exactly one document name.";
		sh:name "Document Name" ; 
		sh:description "name of referenced document"@en ; 
	];
	sh:property[
		sh:path sen-dat:checksum;
		sh:datatype xsd:string;
		skos:example ""; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Optional: Add a checksum.";
		sh:name "Checksum" ; 
		sh:description "checksum of document"@en ; 
	];
	sh:property[
		sh:path sen-dat:fileSize;
		sh:datatype xsd:string;
		skos:example "700 KB"; 
		sh:minCount 0 ;
		sh:maxCount 1 ; 
		sh:message "Optional: Add file size.";
		sh:name "File Size" ; 
		sh:description "Size of referenced document"@en ; 
	];
	sh:property[
		sh:path sen-dat:modelLink;
		sh:or(
			[
				sh:datatype xsd:string;
			]
			[
				sh:datatype xsd:anyURI;
			]
		);
		skos:example "www.documentlink.org"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add a link to the document.";
		sh:name "Document Link" ; 
		sh:description "link referring to an online document"@en ; 
	].
	
