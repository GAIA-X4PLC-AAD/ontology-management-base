### PLCAAD Generic Localization Shape
plcaad-tbd:LocalizationShape
	a sh:NodeShape;
	sh:closed false;
	sh:targetClass plcaad-tbd:Localization;
	sh:property[
		sh:path plcaad-tbd:tag;
		sh:datatype xsd:string;
		skos:example "something"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add exactly one tag.";
		sh:name "PLCAAD-Tag" ; 
		sh:description "Add a dummy string. This Shape checks if we can use it for general properties"@en ; 
	].
	
### PLCAAD Generic Environment Shape
plcaad-tbd:EnvironmentShape
	a sh:NodeShape;
	sh:closed false;
	sh:targetClass plcaad-tbd:Environment;
	sh:property[
		sh:path plcaad-tbd:tag;
		sh:datatype xsd:string;
		skos:example "something"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
