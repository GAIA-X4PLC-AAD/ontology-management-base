		sh:name "Weather Precipation" ; 
		sh:description "Specification of precipitation conditions during data capture"@en ; 
	];
	sh:property[ ###should be an object property to a ModelDocument Class
		sh:path sen-dat:modelLink;
		sh:datatype xsd:anyURI;
		skos:example "e.g. Link to OpenDrive map"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add exactly one link to environment model.";
		sh:name "Model Link" ; 
		sh:description "Link to environment model"@en ; 
	].
	
