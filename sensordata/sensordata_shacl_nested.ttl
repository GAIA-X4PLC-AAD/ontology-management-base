	sh:property[
		sh:path sen-dat:sensorName;
		sh:datatype xsd:string;
		skos:example "ARS548"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add at exactly one sensor name.";
		sh:name "Sensor Name" ; 
		sh:description "Name of Sensor"@en ; 
	];
	sh:property[
		sh:path sen-dat:sensorHardwareVersion;
		sh:datatype xsd:string;
		skos:example "ARS540 HW Rev. x.y"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
