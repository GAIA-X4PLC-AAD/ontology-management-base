		sh:description "???"@en ; 
	];
	sh:property[
		sh:path sen-dat:startCoordinates;
		sh:datatype xsd:string; ###???--> Check range format
		skos:example "???"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add exactly one set of start coordinates.";
		sh:name "Start Coordinates" ; 
		sh:description "Coordinates according to geodetic reference system specification of gps measurement device"@en ; 
	];
	sh:property[
		sh:path sen-dat:endCoordinates;
		sh:datatype xsd:string; ###???--> Check range format
		skos:example "???"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add exactly one set of end coordinates.";
		sh:name "End Coordinates" ; 
		sh:description "Coordinates according to geodetic reference system specification of gps measurement device"@en ; 
	];
	sh:property[
		sh:path sen-dat:gpsOffset;
		sh:datatype xsd:string; ###???--> Check range format
		skos:example "???"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add exactly one GPS Offset.";
		sh:name "GPS Offset" ; 
		sh:description "Offset of gps measurement device from ground projection middle rear axle"@en ; 
	].
	
