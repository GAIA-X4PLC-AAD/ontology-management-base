###SRMD Localization
sen-dat:SRMD_LocalizationShape
	a sh:NodeShape;
	sh:targetClass sen-dat:SRMD_Localization;
	sh:property[
		sh:path sen-dat:country;
		sh:datatype xsd:string;
		skos:example "DE, AT, ... (ISO 3166-1)"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add exactly one Country.";
		sh:name "Localization Country" ; 
		sh:description "Add Country name according to ISO 3166-1"@en ; 
	];
	sh:property[
		sh:path sen-dat:description;
		sh:datatype xsd:string;
		skos:example "Edemissen airfield test site, Testfeld Niedersachsen"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add exactly one Description for Localization";
		sh:name "Localization Description" ; 
		sh:description "Description of the Location: City, region, location..."@en ; 
	];
	sh:property[
		sh:path sen-dat:geodeticReferenceSystem;
		sh:datatype xsd:string;
