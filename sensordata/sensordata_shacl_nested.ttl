		sh:description "Name sensor technology variant"@en ; 
	];
	sh:property[
		sh:path sen-dat:sensorStatus;
		sh:datatype xsd:string;
		skos:example "Released"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add at exactly one sensor status.";
		sh:name "Sensor Status" ; 
		sh:description "Sensor Status"@en ; 
	];	
	sh:property[
		sh:path sen-dat:sensorPurpose;
		sh:datatype xsd:string;
		skos:example "Long Range Radar Sensor w/ Object Detection"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add at exactly one sensor purpose.";
		sh:name "Sensor Purpose" ; 
		sh:description "Sensor Purpose"@en ; 
	];
	### Sensor has exactly 1 SensorSpecification
	sh:property[ 
		sh:path sen-dat:hasSpec;
		sh:node sen-dat:SensorSpecificationShape;
		sh:minCount 1;
		sh:maxCount 1;
		sh:name "Sensor Specification";
		sh:description "Reference to a sensor specification individual for the sensor" @en;
		sh:message "Add a reference to one Sensor Specification object" @en;
		skos:example "prefix:SensorSpecificationIRI";
	];
	### Sensor has min 1 SensorConfiguration
	sh:property[ 
		sh:path sen-dat:hasSensorConfiguration;
		sh:node sen-dat:SensorConfigurationShape;
		sh:minCount 1;
		###sh:maxCount 1;
		sh:name "Sensor Configuration";
		sh:description "Reference to a sensor configuration individual for the sensor" @en;
		sh:message "Add a reference to one Sensor Configuration object" @en;
		skos:example "prefix:SensorConfigurationIRI";
	];
	### Sensor has exactly 1 sensorManufacturer
	sh:property[ 
		sh:path sen-dat:sensorManufacturer;
		sh:node sen-dat:ManufacturerShape;
		sh:minCount 1;
		sh:maxCount 1;
		sh:name "Sensor Manufacturer";
		sh:description "Reference to a manufacturer individual for the sensor" @en;
		sh:message "Add a reference to one Sensor Manufacturer object" @en;
		skos:example "prefix:SensorManufacturerIRI";
	].
	
###Sensor Specification
sen-dat:SensorSpecificationShape
