###Sensor Specification Document
sen-dat:SensorSpecificationDocumentShape
	a sh:NodeShape;
	sh:targetClass sen-dat:SensorSpecificationDocument;
	sh:property[
		sh:path sen-dat:doumentLink;
		sh:or(
			[
				sh:datatype xsd:anyURI;
			]
			[
				sh:datatype xsd:string;
			]
		);
		skos:example "https://www.linkToSomething.org/"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add exactly one Link to specification document.";
		sh:name "Sensor Specification Link" ; 
		sh:description "Add link to sensor specification document"@en ; 
	];
	sh:property[
		sh:path sen-dat:fileName;
		sh:datatype xsd:string;		
		skos:example "ARS548 Sensor Spec"; 
		sh:minCount 1 ;
		sh:maxCount 1 ; 
		sh:message "Add the name of specification document.";
		sh:name "Sensor Specification Document Name" ; 
		sh:description "Add name to sensor specification document"@en ; 
	].
	
