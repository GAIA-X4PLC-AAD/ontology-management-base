@prefix HD-Map: <https://github.com/GAIA-X4PLC-AAD/map-and-scenario-data/HD-Map_ontology> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

HD-Map:Asset_Shape a sh:NodeShape ;
    sh:property [ skos:example "40.0" ;
            sh:datatype xsd:float ;
            sh:description "road network length in km, sum over road length; see ODR spec section 8"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of length failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "length"@en ;
            sh:path HD-Map:HD-Map_quantity_length ],
        [ skos:example "100" ;
            sh:datatype xsd:unsignedInt ;
            sh:description "sum of all objects outlines in the map,  see ODR spec section 11.2"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of number_outlines failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "number_outlines"@en ;
            sh:path HD-Map:HD-Map_quantity_number_outlines ],
        [ skos:example "5" ;
            sh:datatype xsd:unsignedInt ;
            sh:description "sum of all junctions defined in the map,  see ODR spec section 10"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of number_intersections failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "number_intersections"@en ;
            sh:path HD-Map:HD-Map_quantity_number_intersections ],
        [ skos:example "155" ;
            sh:datatype xsd:unsignedInt ;
            sh:description "sum of all traffic signs in the map,  see ODR spec section 12"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of number_traffic_signs failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "number_traffic_signs"@en ;
            sh:path HD-Map:HD-Map_quantity_number_traffic_signs ],
        [ skos:example "3DMS system, Trimble xyz, Riegl xyz" ;
            sh:datatype xsd:string ;
            sh:description "main acquisition device"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of measurement_system failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "measurement_system"@en ;
            sh:path HD-Map:HD-Map_Data_source_measurement_system ],
        [ skos:example "[10,50] km/h" ;
            sh:datatype xsd:string ;
            sh:description "range of speed limits defined in the map, see ODR spec section 9.5.5. "@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of speed_limit failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "speed_limit"@en ;
            sh:path HD-Map:HD-Map_quantity_speed_limit ],
        [ skos:example "Lat: 1.234; Lon 45.23" ;
            sh:datatype xsd:string ;
            sh:description "world coordinates of map origin"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of origin failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "origin"@en ;
            sh:path HD-Map:HD-Map_Georefencing_origin ],
        [ skos:example "trees, street lamps, road patches..." ;
            sh:datatype xsd:string ;
            sh:description "covered object classes, see ODR spec section 11"@en ;
            sh:message "Validation of Level_of_Detail failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "Level_of_Detail"@en ;
            sh:path HD-Map:HD-Map_content_Level_of_Detail ],
        [ skos:example "20" ;
            sh:datatype xsd:float ;
            sh:description "how wide is the area beyond the traffic space modeled"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of range_of_modeling failed!"@en ;
            sh:minCount "0"^^xsd:unsignedInt ;
            sh:name "range_of_modeling"@en ;
            sh:path HD-Map:HD-Map_quantity_range_of_modeling ],
        [ skos:example "2.22" ;
            sh:datatype xsd:float ;
            sh:description "difference of max and mit elevation, extraction from openDrive Element t_road_elevationprofile_elevation, see section 8.4"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of elevation_range failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "elevation_range"@en ;
            sh:path HD-Map:HD-Map_quantity_elevation_range ],
        [ skos:example "0" ;
            sh:datatype xsd:unsignedInt ;
            sh:description "sum of all traffic lights defined in the map,  see ODR spec section 12"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of number_traffic_lights failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "number_traffic_lights"@en ;
            sh:path HD-Map:HD-Map_quantity_number_traffic_lights ],
        [ skos:example "scanner, camera, osm network, aerial images, ..." ;
            sh:datatype xsd:string ;
            sh:description "basic data for the creation of the map"@en ;
            sh:message "Validation of used_data_sources failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "used_data_sources"@en ;
            sh:path HD-Map:HD-Map_Data_source_used_data_sources ],
        [ skos:example "[motorway, residential]" ;
            sh:datatype xsd:string ;
            sh:description "covered/used road types, defined over ODR element t_road_type, see ODR spec section 8.3"@en ;
            sh:message "Validation of road_types failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "road_types"@en ;
            sh:path HD-Map:HD-Map_content_road_types ],
        [ skos:example "shoulder, curb, ..." ;
            sh:datatype xsd:string ;
            sh:description "covered lane types, see ODR spec section 9.5.3."@en ;
            sh:message "Validation of lane_types failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "lane_types"@en ;
            sh:path HD-Map:HD-Map_content_lane_types ],
        [ skos:example "EPSG xyz" ;
            sh:datatype xsd:string ;
            sh:description "ellipsodial height or orthometric height"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of height_system failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "height_system"@en ;
            sh:path HD-Map:HD-Map_Georefencing_height_system ],
        [ skos:example "right-hand traffic" ;
            sh:datatype xsd:string ;
            sh:description "traffic direction, i.e. right-hand or left-hand traffic"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of traffic_direction failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "traffic_direction"@en ;
            sh:path HD-Map:HD-Map_general_traffic_direction ],
        [ skos:example "0.1" ;
            sh:datatype xsd:float ;
            sh:description "accuracy of traffic relevant objects, signs and signals"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of accuracy_signals failed!"@en ;
            sh:minCount "0"^^xsd:unsignedInt ;
            sh:name "accuracy_signals"@en ;
            sh:path HD-Map:HD-Map_quality_accuracy_signals ],
        [ skos:example "0.1" ;
            sh:datatype xsd:float ;
            sh:description "accuracy of lane modell 2d"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of accuracy_lane_model_2d failed!"@en ;
            sh:minCount "0"^^xsd:unsignedInt ;
            sh:name "accuracy_lane_model_2d"@en ;
            sh:path HD-Map:HD-Map_quality_accuracy_lane_model_2d ],
        [ skos:example "200" ;
            sh:datatype xsd:unsignedInt ;
            sh:description "sum of all objects in the map,  see ODR spec section 11"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of number_objects failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "number_objects"@en ;
            sh:path HD-Map:HD-Map_quantity_number_objects ],
        [ skos:example "0.1" ;
            sh:datatype xsd:float ;
            sh:description "accuracy of objects in the traffic space, which do not directly affect the traffic"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of accuracy_objects failed!"@en ;
            sh:minCount "0"^^xsd:unsignedInt ;
            sh:name "accuracy_objects"@en ;
            sh:path HD-Map:HD-Map_quality_accuracy_objects ],
        [ skos:example "0.01" ;
            sh:datatype xsd:float ;
            sh:description "precision of measured road network (relative accuracy)"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of precision failed!"@en ;
            sh:minCount "0"^^xsd:unsignedInt ;
            sh:name "precision"@en ;
            sh:path HD-Map:HD-Map_quality_precision ],
        [ skos:example "0.1" ;
            sh:datatype xsd:float ;
            sh:description "accuracy lane modell height"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of accuracy_lane_model_height failed!"@en ;
            sh:minCount "0"^^xsd:unsignedInt ;
            sh:name "accuracy_lane_model_height"@en ;
            sh:path HD-Map:HD-Map_quality_accuracy_lane_model_height ],
        [ skos:example "2022-04-01 00:00:00" ;
            sh:datatype xsd:string ;
            sh:description "time of data acquisition used to generate the map, if partial measurement: oldest date"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of recording_time failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "recording_time"@en ;
            sh:path HD-Map:HD-Map_general_recording_time ],
        [ skos:example "EPSG 32632" ;
            sh:datatype xsd:string ;
            sh:description "EPSG code of the map"@en ;
            sh:maxCount "1"^^xsd:unsignedInt ;
            sh:message "Validation of projection_type failed!"@en ;
            sh:minCount "1"^^xsd:unsignedInt ;
            sh:name "projection_type"@en ;
            sh:path HD-Map:HD-Map_Georefencing_projection_type ] ;
    sh:targetClass HD-Map:Asset .

