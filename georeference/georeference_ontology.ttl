@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix georeference: <https://github.com/GAIA-X4PLC-AAD/ontology-management-base/tree/main/georeference/> .
@prefix gax-core: <https://w3id.org/gaia-x/core#> .
@prefix gx: <https://registry.lab.gaia-x.eu/development/api/trusted-shape-registry/v1/shapes/jsonld/trustframework#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

georeference: a owl:Ontology ;
    rdfs:label "ontology definition for location and georeference information "@en ;
    dcterms:contributor "Sebastian Tuttas (3DMS)" ;
    owl:versionInfo "0.1" .

georeference:Georeference a owl:Class ;
    rdfs:label "class definition for Georeference" ;
    rdfs:comment "Attributes for location and georeference information"@en .

georeference:GeodeticReferenceSystem a owl:Class ;
    rdfs:label "class definition for GeodeticReferenceSystem" ;
    rdfs:comment "Attributes for GeodeticReferenceSystem"@en .

georeference:BoundingBox a owl:Class ;
    rdfs:label "class definition for BoundingBox" ;
    rdfs:comment "Attributes for BoundingBox"@en .

georeference:Coordinate2D a owl:Class ;
    rdfs:label "class definition for Coordinate2D" ;
    rdfs:comment "Attributes for Coordinate2D"@en .

georeference:ProjectLocation a owl:Class ;
    rdfs:label "class definition for Georeference" ;
    rdfs:comment "Attributes for Georeference"@en .
